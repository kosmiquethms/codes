<!--============================== scripts ==============================-->
<script>
$(document).ready(function(){
    
    $('body').addClass('loaded');
    
    // posts counter 
    var url = "https://{name}.tumblr.com/api/read/json";
    $.getScript(url,function(){
        pt = tumblr_api_read["posts-total"].toLocaleString('en-US');
        $('.total-posts').text(pt + " posts");
    });
    
    // chat label
    $(".chat .label").text(function () {return $(this).text().replace(":", ""); });

    // custom audio
    customAudio({
    	post: ".post",
    	default: false,
    	pauseAll: true,
    	playButton: "<i data-feather='play'></i>",
    	pauseButton: "<i data-feather='pause'></i>",
    	errorIcon: "<i data-feather='x'></i>",
    	hideInfoIfError: true,
    	callAfterLoad: () => {feather.replace();},
    });
    
    //shorten notes counter - modified by kosmique
    $('article').find('.notecount span').each(function(){
       var n = $(this).html().split(' ')[0].replace(/,/g, '');
       if (n > 999 && n <= 999999) {
           n = Math.floor(n / 100) / 10;
           $(this).text(n + 'k');
       }
       
       if (n > 999999) {
           n = Math.floor(n / 100000) / 10;
           $(this).text(n + 'kk');
       }
    });
    
    // captions
    $('.captions').each(function() {
        
        if ($(this).children('.comment').length > 1) {
            $(this).addClass('chain');
        }
        
        if($(this).hasClass('chain')){
            $(this).parent('.post').find('.photo, .audio, .link').wrap('<div class="has-chain"></div>')
            $(this).append('<div class="line"></div>')
        }
    });
    
    $('.post').each(function() {$(this).find('a.read_more').parent('p').addClass('has-truncation');});
    
    $('.has-chain').each(function() {
        $(this).append('<div class="line"></div>');
        var $line = $(this).find('.line').parent().height() + 35
         $(this).find('.line').height(function(i,x){
            return x + $line;
        });
    });
    
    // https://forum.jquery.com/topic/get-parent-height-and-assign-to-selector#14737000002190425
    
    $('.line').each(function(){
        
        var $parent = $(this).parent().height()
        var $comment = $(this).parent().find('.comment:last-of-type').height()
        var $title = $(this).parents('.post').find('.post-title').height()
        
        if (!$(this).parents('.post').hasClass("has-title")) {
            var parentHeight = $parent - $comment;  
            $(this).height(function(i,x){
                return x + parentHeight;
            });
        }
        
        if ($(this).parents('.post').hasClass("has-title")) {
            var parentHeight = $parent - $comment + $title + 25;  
            $(this).height(function(i,x){
                return x + parentHeight;
            });
            
            $(this).css('top', -$title - 35)
        }
    });   

    
    // photoset
    $('.photo-slideshow').pxuPhotoset({
        lightbox: true,
        rounded: false,
        gutter: '1px',
        borderRadius: '0px',
        photoset: '.photo-slideshow',
        photoWrap: '.photo-data',
        photo: '.pxu-photo'
    });
    
    // feathericons
    feather.replace();
        
    // flexible frames
    function flexFrame() {
       $(".captions").each(function() {
            $(this).find("iframe").wrap("<div class='capframe'></div>"); // wrap iframe 
            flexibleFrames($(".capframe"));
        });
        flexibleFrames($(".video"));
    }
    
    $(document).ready(flexFrame);
    
    // tooltips
    tippy("[title]", {
        theme: "kosmique",
        duration: 300,
        allowHTML: true,
        arrow: false,
        animation: 'scale-subtle',
        followCursor: true,
        interactive: false,
        inlinePositioning: false,
        maxWidth: 300,
        offset: [0, 20],
        zIndex: 9999999999999,
        placement: "bottom",
        content(reference) {
            const title = reference.getAttribute("title");
            reference.removeAttribute("title");
            return title
        },
    });
	
    // remove blank captions
    $('.captions').each(function() {
        var $this = $(this);
        if($this.html().replace(/\s|&nbsp;/g, '').length == 0)
            $this.remove();
    });
    
    //back to top
    $(document).scroll(function() {
        var y = $(this).scrollTop();
        if (y > 200) {
            $('.back').fadeIn();
        } else {
            $('.back').fadeOut();
        }
        
        if (y > $('.header-image').height()) {
            $('#sticky').addClass('active');
            $('#sticky, aside .open-search').fadeIn();
        } else {
            $('#sticky').removeClass('active');
            $('#sticky, aside .open-search').fadeOut();
        }
    });

    $(".back").click(function() {
        $("html, body").animate({scrollTop: 0}, 1000);
        return false;
    });
    
    // tumblr controls
    $('.open-controls').click(function(){
        $(this).toggleClass('active')
        $('.tmblr-iframe').toggleClass('active');
		return false
    }),
    $('.open-search').click(function(){
        $('#sticky .blog-icon, #sticky .blog, .search').toggle();
        return false; 
    });
    
	// time ago plugin
    $('.timeago a').timeAgo({
        time: "short", // can be "letter" "short" or "word"
        spaces: true, // adds spaces between words and numbers
        words: false, // turns numbers to words
        prefix: "", // adds a prefix to the outputted string. could be "~" or "about"
        suffix: "", // adds a suffix to the outputted string. could be "ago"
    });
    
    //npf photoset
    npfPhotosets(".post", {
        includeCommonPhotosets: false,
        photosetMargins:"1"
    });
    
    $('.tabs a[data-section], aside a[data-section]').click(function(){
        var divID = $(this).attr('data-section');
        $('.tabs').find('a[data-section="' + divID + '"]').addClass('active');
        $('main > section#' + divID).show();
        $('main > section:not(#' + divID +')').hide();
        $('.tabs a:not([data-section="' + divID + '"])').removeClass('active');
        return divID 
    });
    
    //status bar
    $('.bar').each(function(){
        var width = $(this).attr('data-width');
        $(this).width(width);
        $(this).next('.bar-percent').text(width);
    });
    
    {block:ifInfiniteScroll}
        {block:indexpage}
            //infinite scroll
            var $container = $('#posts').infiniteScroll({
                // options
                path: '#pagination .next',
                append: '.post',
                history: false,
                hideNav: '#pagination',
                {block:ifManualLoad}
                    scrollThreshold: false,
                    button: '.load',
                    status: '.load-status'
                {/block:ifManualLoad}
            });
            
            $container.on( 'append.infiniteScroll', function( e, response, path, items ) {
                var $newElems = $( items );
                
                var $newElemsIDs = $newElems.map(function(){
                    return this.id;
                }).get();
                Tumblr.LikeButton.get_status_by_post_ids($newElemsIDs);
                
                $newElems.find('.photo-slideshow').pxuPhotoset({
                    lightbox: true,
                    rounded: false,
                    gutter: '1px',
                    photoset: '.photo-slideshow',
                    photoWrap: '.photo-data',
                    photo: '.pxu-photo'
                });
                
                $newElems.find('.notecount span').each(function(){
                    var n = $(this).html().split(' ')[0].replace(/,/g, '');
                    if (n > 999 && n <= 999999) {
                        n = Math.floor(n / 100) / 10;
                        $(this).text(n + 'k');
                    }
                    
                    if (n > 999999) {
                        n = Math.floor(n / 100000) / 10;
                        $(this).text(n + 'kk');
                    }
                });
                
                feather.replace();
                
                $(document).ready(flexFrame);
                
                $('.post').each(function() {$(this).find('a.read_more').parent('p').addClass('has-truncation');});
                
                $newElems.find('.captions').each(function() {
        
                    if ($(this).children('.comment').length > 1) {
                        $(this).addClass('chain');
                    }
                    
                    if($(this).hasClass('chain')){
                        $(this).parent('.post').find('.photo, .audio, .link').wrap('<div class="has-chain"></div>')
                        $(this).append('<div class="line"></div>')
                    }
                });
                
                $newElems.find('.has-chain').each(function() {
                    $(this).append('<div class="line"></div>');
                    var $line = $(this).find('.line').parent().height() + 35
                     $(this).find('.line').height(function(i,x){
                        return x + $line;
                    });
                });
                
                $newElems.find('.line').each(function(){
                    
                    var $parent = $(this).parent().height()
                    var $comment = $(this).parent().find('.comment:last-of-type').height()
                    var $title = $(this).parents('.post').find('.post-title').height()
                    
                    if (!$(this).parents('.post').hasClass("has-title")) {
                        var parentHeight = $parent - $comment;  
                        $(this).height(function(i,x){
                            return x + parentHeight;
                        });
                    }
                    
                    if ($(this).parents('.post').hasClass("has-title")) {
                        var parentHeight = $parent - $comment + $title + 25;  
                        $(this).height(function(i,x){
                            return x + parentHeight;
                        });
                        
                        $(this).css('top', -$title - 35)
                    }
                });   
                
                tippy("[title]", {
                    theme: "kosmique",
                    duration: 300,
                    allowHTML: true,
                    arrow: false,
                    animation: 'scale-subtle',
                    followCursor: true,
                    interactive: false,
                    inlinePositioning: false,
                    maxWidth: 300,
                    offset: [0, 20],
                    zIndex: 9999999999999,
                    placement: "bottom",
                    content(reference) {
                        const title = reference.getAttribute("title");
                        reference.removeAttribute("title");
                        return title
                    },
                });
            	
                $newElems.find('.captions').each(function() {
                    var $this = $(this);
                    if($this.html().replace(/\s|&nbsp;/g, '').length == 0)
                        $this.remove();
                });
                
                customAudio();
                
                $newElems.find('.timeago a').timeAgo({
                    time: "short", // can be "letter" "short" or "word"
                    spaces: true, // adds spaces between words and numbers
                    words: false, // turns numbers to words
                    prefix: "", // adds a prefix to the outputted string. could be "~" or "about"
                    suffix: "", // adds a suffix to the outputted string. could be "ago"
                });
                
                npfPhotosets(".post", {
                    includeCommonPhotosets: false,
                    photosetMargins:"1"
                });
            });
            
            {block:ifManualLoad}
                $container.on( 'request.infiniteScroll', function( event, path, fetchPromise ) {$('.load-button').hide();    });
                
                $container.on( 'load.infiniteScroll', function( event, body, path, response ) {$('.load-button').show();    });
                
            {/block:ifManualLoad}
        {/block:indexpage}
    {/block:ifInfiniteScroll}
});

// remove tumblr's redirects
function noHrefLi(){
    var linkSet = document.querySelectorAll('a[href*="href.li/?"]');
    Array.prototype.forEach.call(linkSet,function(el,i){
        var theLink = linkSet[i].getAttribute('href').split("href.li/?")[1];
        linkSet[i].setAttribute("href",theLink);
    });
}
noHrefLi();

</script>
</body></html>
