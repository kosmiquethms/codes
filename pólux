            {block:ifLinkFourTitle}
                <a href="{text:link four url}">
                    {text:link four title}
                </a>
            {/block:ifLinkFourTitle}
            
            {block:HasPages}
                {block:Pages}
                    <a href="{URL}">{Label}</a>
                {/block:Pages}
            {/block:HasPages}
        </nav>
    </div>
    
    {block:indexpage}
        <nav class="tabs">
            <a href="#" data-section="content" class="active">
                posts
            </a>
            
            {block:ifMessageSection}
                <a href="#" data-section="message">
                    message
                </a>
            {/block:ifMessageSection}
            
            {block:ifAboutSection}
                <a href="#" data-section="about">
                    about
                </a>
            {/block:ifAboutSection}
            
            <a href="/archive">
                archive
            </a>
        </nav>
    {/block:indexpage}
</header>

