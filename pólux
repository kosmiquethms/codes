    	</nav>
    	
    	{block:ifManualLoad}
        	<footer class="loader">
            	<div class="load-button">
            	    <a class="load">load more content</a>
            	</div>
            	
            	<div class="load-status" style="display:none!important;">
                    <p class="infinite-scroll-request">
                        <span></span>
                    </p>
                    
                    <p class="infinite-scroll-last">end of content</p>
                    
                    <p class="infinite-scroll-error">could not load</p>
                </div>
            </footer>
        {/block:ifManualLoad}
    {/block:IfInfiniteScroll}
{/block:Pagination}
</section> <!-- #entries -->










