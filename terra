        <line x1="3" y1="3" x2="21" y2="21"></line>
        <path d="M17 17h-13a4 4 0 0 0 2 -3v-3a7 7 0 0 1 1.279 -3.716m2.072 -1.934c.209 -.127 .425 -.244 .649 -.35a2 2 0 1 1 4 0a7 7 0 0 1 4 6v3"></path>
        <path d="M9 17v1a3 3 0 0 0 6 0v-1"></path>
    </svg>
</a>

<div class="updates" style="display:none;">
    <div class="title">updates</div>
    
    <div class="update-body">
        {block:ifUpdateText}
            <section>
                {text:update text}
            </section>
        {/block:ifUpdateText}
        
        <section class="status-section">
            {block:ifFirstStatusTitle}
                <li class="status">
                    <span class="square-status" mode="{select:first status mode}"></span>
                    <span class="status-title">
                        {text:First status title}
                    </span>
                </li>
            {/block:ifFirstStatusTitle}
            
            {block:ifSecondStatusTitle}
                <li class="status">
                    <span class="square-status" mode="{select:second status mode}"></span>
                    <span class="status-title">
                        {text:Second status title}
                    </span>
                </li>
            {/block:ifSecondStatusTitle}
            
            {block:ifThirdStatusTitle}
                <li class="status">
                    <span class="square-status" mode="{select:third status mode}"></span>
                    <span class="status-title">
                        {text:Third status title}
                    </span>
                </li>
            {/block:ifThirdStatusTitle}
            
            {block:ifFourthStatusTitle}
                <li class="status">
                    <span class="square-status" mode="{select:fourth status mode}"></span>
                    <span class="status-title">
                        {text:Fourth status title}
                    </span>
                </li>
            {/block:ifFourthStatusTitle}
        </section>
    </div>
</div>
{/block:ifUpdates}

