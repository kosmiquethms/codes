<script src="https://unpkg.com/masonry-layout@4/dist/masonry.pkgd.min.js"></script>
<script src="https://unpkg.com/imagesloaded@4/imagesloaded.pkgd.min.js"></script>

<script>
$(document).ready(function(){
    
    resize();
    $(window).resize(function() {resize();});
    
    
    function resize() {
        var $screenWidth = $(window).width();
        if ($screenWidth < 1024) {
            $('#posts').masonry('destroy');
            $('.captions').removeClass('long-caption');
            $('.long-caption-link').remove();
        }
        if ($screenWidth >= 1024) {
            // masonry
            $('#posts').masonry({
                itemSelector: ".post",
                initLayout: 1,
                percentPosition: true,
            });
            $('#posts').on("layoutComplete", function(o, r) {
                console.log(r.length)
            }), $('#posts').masonry({
                fitWidth: false,
                horizontalOrder: 0,
                gutter: 50,
                resize: 1,
            }), $('#posts').imagesLoaded().progress(function() {
                $('#posts').masonry("layout");
            });
            
            $('article[post-type="text"]').find('.captions').each(function(){
                var $this = $(this);
                var heightbody = $this.innerHeight(),
                    buttontext = "read more",
                    readmore = "long-caption-link";
                if (heightbody >= 500) {
                    var permalinkPage = $(this).parents('article.post').find('.notecount').attr("href");
                    $(this).addClass('long-caption');
                    $this.append('<a href="#" class="' + readmore + '">' + buttontext + '</a>');
                    $(this).parents('.post').find('.long-caption-link').attr("href", permalinkPage);
                    $('#posts').masonry();
               }
            });
        }
    }
    
    
});
</script>
{/block:homepage}
{/block:ifGridPosts}

